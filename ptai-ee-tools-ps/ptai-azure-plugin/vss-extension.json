{
	"manifestVersion": 1,
	"id": "ptai",
	"publisher": "ptsecurity",
	"version": "0.4.36",
	"name": "PT Application Inspector",
	"description": "Add application security testing inside your build process",
	"public": false,
	"categories": ["Build and release"],
	"icons": {
		"default": "logo.png"
	},
	"targets": [{
			"id": "Microsoft.VisualStudio.Services"
		}
	],
	"tags": [
		"Security",
		"SCA",
		"Static Code Analysis",
		"Scan",
		"SAST",
		"DAST",
		"IAST",
		"Positive Technologies",
		"PT AI",
		"Software Composition Analysis",
		"Configuration Analysis"
	],
	"scopes": [
		"vso.build_execute"
	],
	"content": {
		"details": {
			"path": "overview.md"
		}
	},
	"links": {
		"getstarted": {
			"uri": "https://www.ptsecurity.com/ww-en/products/ai/#resources"
		}
	},
	"files": [
		{
			"path": "AI"
		}, {
			"path": "node_modules/vss-web-extension-sdk/lib",
			"addressable": true,
			"packagePath": "lib"
		}
	],
	"contributions": [
		{
			"id": "ptai-scan-task",
			"type": "ms.vss-distributed-task.task",
			"targets": [
				"ms.vss-distributed-task.tasks"
			],
			"properties": {
				"name": "AI"
			}
		}
	]
}
