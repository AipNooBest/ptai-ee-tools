<?xml version="1.0" encoding="UTF-8"?>
<service>
    <id>AI.Enterprise.IntegrationService</id>
    <name>AI.Enterprise.IntegrationService</name>
    <description>PT AI EE Integration Service</description>
    <executable>java</executable>
    <arguments>-Xmx2g -Xms32m -XX:+UseConcMarkSweepGC -Dspring.main.lazy-initialization=true
        -Dlogging.level.com.ptsecurity.appsec.ai.ee.utils.ci.integration.service=TRACE -jar
        ptai-integration-service.jar 10
    </arguments>
    <depend>Consul</depend>
    <depend>AI.DescriptionsService</depend>
    <depend>AI.Enterprise.AuthService</depend>
    <depend>AI.Enterprise.ChangeHistory</depend>
    <depend>AI.Enterprise.FileContent.API</depend>
    <depend>AI.Enterprise.Gateway</depend>
    <depend>AI.Enterprise.IssueTracker</depend>
    <depend>AI.Enterprise.NotificationsService</depend>
    <depend>AI.Enterprise.Projects.API</depend>
    <depend>AI.Enterprise.SettingsProvider.API</depend>
    <depend>AI.Enterprise.SystemManagement</depend>
    <depend>AI.Enterprise.UI</depend>
    <depend>AI.Enterprise.UpdateServer</depend>
    <startmode>Manual</startmode>
</service>